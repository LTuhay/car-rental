@page
@model Car_rental.Pages.Account.SignOutModel
@{
    ViewData["Title"] = "Sign Out";
}

<h2>Sign Out</h2>

<p>You are about to sign out. If you wish to proceed, please click the button below.</p>

<form method="post">
    <button type="submit" class="btn btn-primary">Sign Out</button>
</form>

<p>
    <a asp-page="/Index" class="btn btn-secondary">Cancel</a>
</p>
